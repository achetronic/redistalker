{
    "name": "snsorial/tranzformer",
    "type": "project",
    "description": "An strange MQTT server who uses Redis for queues",
    "keywords": [
        "mqtt",
        "redis",
        "tranzformer"
    ],
    "license": "MIT",
    "require": {
        "codedungeon/php-cli-colors": "^1.12",
        "predis/predis": "^1.1",
        "promphp/prometheus_client_php": "^2.2",
        "react/event-loop": "^1.1.1",
        "react/http": "^1.2",
        "react/socket": "^1.6",
        "symfony/dotenv": "^5.2"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
